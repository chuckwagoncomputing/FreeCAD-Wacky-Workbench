	doc.recompute()
	p2.Length = 60.000000
	p2.TaperAngle = 0.000000
	p2.UseCustomVector = 0
	p2.Direction = (0, 0, 1)
	p2.ReferenceAxis = (s2, ['N_Axis'])
	p2.AlongSketchNormal = 1
	p2.Type = 0
	p2.UpToFace = None
	p2.Reversed = 0
	p2.Midplane = 0
	p2.Offset = 0
	doc.recompute()
	s2.Visibility = False
	return b2

def setUpBodyBase(doc):
