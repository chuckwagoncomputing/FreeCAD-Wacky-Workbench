	p1.UseCustomVector = 0
	p1.Direction = (0, 0, 1)
	p1.ReferenceAxis = (s1, ['N_Axis'])
	p1.AlongSketchNormal = 1
	p1.Type = 0
	p1.UpToFace = None
	p1.Reversed = 0
	p1.Midplane = 0
	p1.Offset = 0
	doc.recompute()
	s1.Visibility = False
	return b1

